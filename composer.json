{
  "name": "fayez/calculator-bundle",
  "type": "symfony-bundle",
  "description": "Bundle for summing up two integers",
  "keywords": ["Sum", "Calculation", "Calculator"],
  "homepage": "https://github.com/fayez/FayezCalculatorBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Fayez Ghazzawi",
      "email": "fayez.ghazzawi@multibankfx.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "symfony/framework-bundle": ">=4.0",
    "symfony/yaml": ">=4.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": ">=4.0"
  },
  "config": {
    "platform": {
      "php": "7.2"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Fayez\\CalculatorBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Fayez\\CalculatorBundle\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "<4.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}